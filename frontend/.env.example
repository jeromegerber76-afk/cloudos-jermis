# CloudOS.Jermis Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_VERSION=v1

# Application Configuration
REACT_APP_NAME="CloudOS.Jermis"
REACT_APP_VERSION=1.0.0

# Authentication Configuration
REACT_APP_AUTH_TOKEN_KEY=auth_token
REACT_APP_SESSION_TIMEOUT=7200000

# Feature Flags
REACT_APP_ENABLE_REGISTRATION=true
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_DARK_MODE=false

# External Services (optional)
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=

# Development Settings
GENERATE_SOURCEMAP=true
SKIP_PREFLIGHT_CHECK=true